#:import utils kivy.utils
<DataAssetsPropertiesWindow>
    name: "data-assets"
    BoxLayout:
        padding: 10
        spacing: 20
        orientation: "vertical"
        size: root.width, root.height
        size_hint_x: None
        GridLayout:
            cols:2
            padding: 10
            spacing: 20
            size_hint: None, None
            height: self.minimum_height
            width: self.parent.width
            HeaderLabel:
                text: "Data-assets"
            MySpinner:
                id: methodDataAssets
                text: "Choose a Method"
                values: ['New Batch','Modify']

        ScrollView:
            canvas:
                Color:
                    rgba: 1,1,1, .9
                Line:
                    width: 2.
                    rectangle: (self.x, self.y, self.width, self.height)
            pos_hint:{'center_x':0.5,'center_y':0.5}
            do_scroll_x: False
            do_scroll_y: True
            BoxLayout:
                canvas:
                    Color:
                        rgba: 1,1,1, .9
                    Line:
                        width: 2.
                        rectangle: (self.x, self.y, self.width, self.height)
                orientation: "vertical"
                size_hint: None, None
                size: (self.parent.width, 500)
                GridLayout:
                    canvas:
                        Color:
                            rgba: 1,1,1, .9
                        Line:
                            width: 2.
                            rectangle: (self.x, self.y, self.width, self.height)
                    cols:2
                    row_force_default: True
                    row_default_height: 50
                    BatchGrid:
                        canvas:
                            Color:
                                rgba: 1,1,1, .9
                            Line:
                                width: 2.
                                rectangle: (self.x, self.y, self.width, self.height)
                        BatchUserInputLabel:
                            canvas:
                                Color:
                                    rgba: 1,1,1, .9
                                Line:
                                    width: 2.
                                    rectangle: (self.x, self.y, self.width, self.height)
                            text: "id:"
                        BatchUserInput:
                            canvas:
                                Color:
                                    rgba: 1,1,1, .9
                                Line:
                                    width: 2.
                                    rectangle: (self.x, self.y, self.width, self.height)
                            id: batchId
                            text: 'data-assets'

                    UserInputBox:
                        canvas:
                            Color:
                                rgba: 1,1,1, .9
                            Line:
                                width: 2.
                                rectangle: (self.x, self.y, self.width, self.height)
                        spacing: 0
                        padding: 0
                        BatchUserInputLabel:
                            text: "QA Batch?:"
                        MyCheckBox:
                            id: QaBatch
                            active: False

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "startingTime:"
                        BatchUserInput:
                            id: startingTime
                            text: '11:01:00'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "endingTime:"
                        BatchUserInput:
                            id: endingTime
                            text: '13:00:00'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "excludeDays:"
                        BatchUserInput:
                            id: excludeDays

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "recurrencePattern:"
                        BatchUserInput:
                            id: recurrencePattern
                            text: 'MINUTES'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "flowId:"
                        BatchUserInput:
                            id: flowId
                            text: 'data-assets-flow'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "maxResult:"
                        BatchUserInput:
                            id: maxResult
                            text: '50'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "importPagingSize:"
                        BatchUserInput:
                            id: importPagingSize
                            text: '50'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "threadSplitter:"
                        BatchUserInput:
                            id: threadSplitter
                            text: '1'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "importMethods:"
                        BatchMySpinner:
                            id: importMethods
                            text: "whiteList"
                            values: 'whiteList','autoRegister', 'userSettings'
                            on_text: root.methods_update(importMethods.text)

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "taskType:"
                        BatchMySpinner:
                            id: taskType
                            text: "schedule"
                            values: 'schedule','adhoc'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "intervalInMin:"
                        BatchUserInput:
                            id: intervalInMin
                            text: '20160'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "pushNotificationOptIn:"
                        BatchUserInput:
                            id: pushNotificationOptIn
                            text: 'True'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "groupName:"
                        BatchUserInput:
                            id: groupName

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "channel:"
                        BatchUserInput:
                            id: channel

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "autoGenerate:"
                        BatchUserInput:
                            id: autoGenerate
                            text: 'False'

                    BatchGrid:
                    BatchGrid:
                        BatchUserInputLabel:
                            text: "Auto register API:"
                        BatchMySpinner:
                            id: API
                            disabled: True
                            text: "getInsights"
                            values: 'getInsights','genereateInsights', 'executeFlow', 'getNumberOfInsights'

                    BatchGrid:
                        BatchUserInputLabel:
                            text: "Context for this API:"
                        BatchUserInput:
                            id: Context
                            disabled: True
                            text: 'insightsDefault'


        #menu
        tailBoxLayout:
            canvas:
                Color:
                    rgba: 1,1,1, .9
                Line:
                    width: 2.
                    rectangle: (self.x, self.y, self.width, self.height)
            RoundedButton:
                text: "Back"
                on_release:
                    app.root.current = "batches"
                    root.manager.transition.direction = "right"

            RoundedButton:
                text: "Help"
                on_release: Factory.MenuWindow().openKB(root)

            RoundedButton:
                text: "Run"
                on_release: Factory.batchesWindow.runFunc(root)



